{#
    on hérite du template "base.html.twig"
    ça permet de récupérer la structure html de ce fichier
    mais également de remplcer le contenu des blocs
#}

{% extends "base.html.twig" %}

{# On écrase le bloc titre#}
{% block title %}
    Bienvenue !
{% endblock %}

{# On écrase le bloc body#}

{% block body%}
    <h1>Bienvenue !</h1>
    <p>
        Page d'accueil <br>
        Pseudo : {{ pseudo }} <br>
        Age : {{ age }} <br>
        
    <ul>
        {% for element in liste  %}
            <li>{{ element }}</li>
        {% endfor %}
    </ul>

{% endblock %}
